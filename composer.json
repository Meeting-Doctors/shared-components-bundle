{
    "name": "gilsegura/shared-bundle",
    "description": "This is a bundle integrating the [shared](https://github.com/gilsegura/shared) library into Symfony applications.",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "symfony/messenger": "^7.0",
        "symfony/amqp-messenger": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/console": "^7.0",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/orm": "^2.17",
        "gilsegura/shared": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49",
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^1.0",
        "phpstan/phpstan-symfony": "^1.3",
        "matthiasnoback/symfony-dependency-injection-test": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "SharedBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SharedBundle\\Tests\\": "tests/"
        }
    }
}
